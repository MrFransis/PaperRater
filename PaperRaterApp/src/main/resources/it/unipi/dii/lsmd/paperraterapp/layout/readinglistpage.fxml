<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="473.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unipi.dii.lsmd.paperraterapp.controller.ReadingListPageController">
   <children>
      <HBox prefHeight="32.0" prefWidth="640.0" style="-fx-border-width: 1px; -fx-border-color: lightgrey;">
         <children>
            <ImageView fx:id="backIcon" fitHeight="36.0" fitWidth="57.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/back.png" />
               </image>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </ImageView>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="383.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
         <children>
            <HBox alignment="TOP_CENTER" layoutX="81.0" prefHeight="29.0" prefWidth="485.0" spacing="10.0">
               <children>
                  <Label fx:id="readingListTitle" alignment="CENTER" style="&#10;" text="Reading List Title" textAlignment="CENTER" textFill="#100000" wrapText="false">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <Label alignment="CENTER" style="&#10;" text="by" textAlignment="CENTER" textFill="#100000" wrapText="false">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <Label fx:id="username" alignment="CENTER" style="&#10;" text="Username" textAlignment="CENTER" textFill="#100000" wrapText="false">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <ScrollPane layoutX="34.0" layoutY="31.0" prefHeight="372.0" prefWidth="567.0">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0">
                     <children>
                        <VBox fx:id="papersBox" />
                     </children>
                  </AnchorPane>
               </content>
            </ScrollPane>
         </children>
      </AnchorPane>
   </children>
</VBox>
